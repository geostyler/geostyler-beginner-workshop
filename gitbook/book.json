{
  "gitbook": "^2.3.0",
  "title": "GeoStyler - Workshop",
  "description": "GeoStyler - workshop materials",
  "language": "en",
  "variables": {
    "workshopName": "GeoStyler - Workshop",
    "workshopRawSourceBaseUrl": "https://rawgit.com/geostyler/geostyler-workshop/master/src/",
    "workshopDownloadUrl": "https://geostyler.github.io/geostyler-workshop/gitbook/geostyler-ws.zip",
    "geoStylerVersion": "12.1.0",
    "reactAppName": "geostyler-app",
    "authors": [{
      "name": "Jan Suleiman",
      "mail": "suleiman@terrestris.de"
    }]
  },
  "plugins": [
    "advanced-emoji",
    "expandable-chapters",
    "image-captions",
    "richquotes",
    "sectionx"
  ],
  "pluginsConfig": {
    "image-captions": {
      "caption": "_CAPTION_"
    },
    "richquotes": {
      "task": {
        "alert": "info",
        "picto": "fa-file-code-o"
      },
      "exercise": {
        "alert": "info",
        "picto": "fa-file-code-o"
      }
    }
  },
  "styles": {
    "website": "style.css"
  }
}
